{
  "manifest_version": 3,
  "name": "Auto Containers",
  "version": "1.0",
  "description": "Testing",
  "permissions": [
    "contextualIdentities",
    "tabs",
    "cookies",
    "storage",
    "webRequest",
    "webRequestBlocking"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "48": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.nationstates.net/*"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["index.js", "request.js"]
  }
}
