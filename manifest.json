{
  "manifest_version": 3,
  "name": "Auto Containers",
  "version": "0.01",
  "description": "Testing",
  "permissions": [
    "activeTab",
    "contextualIdentities",
    "tabs",
    "<all_urls>",
    "cookies"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["index.js"]
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
